            <string>// clear path
path_clear_points(movementPath);

if(endPath == "ready"){
    state = "idle";

    // if actor has actions remaining, become cursor's selectedActor
    if(actions &gt; 0){
        oCursor.selectedActor = id;
        movement_range(map[gridX, gridY], move, actions)
        
        if(canAct){
            switch(attackType){ 
                case "ranged":
                    ranged_attack_range(id);
                    break;
                case "melee":
                    melee_attack_range(id);
                    break;
